<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
	@Styles.Render("~/Content/css/jqueryui")
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="reg-form-background-color">    
    
    <div class="ui basic page stackable grid overview segment">     
    <div class="ui four wide column"></div>
    <div class="sixteen wide column">        
        <div class="ui grid">
            <div class="six wide column">
                <br />
                <br />
                <br />
                <img src="~/Content/images/enter.png" class="ui fluid image" />
            </div>
            <div class="eight wide column">                
                @RenderBody()
            </div>     
        </div>
    </div>
  </div>

    
	@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/semantic")
    @Scripts.Render("~/bundles/common")
    @RenderSection("scripts", required: false)
</body>
</html>